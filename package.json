{
  "name": "grass-bot",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "commonjs",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/main.js",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "format": "prettier --write \"**/*.{ts,md,json}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.9",
    "@octokit/graphql": "^4.8.0",
    "discord-api-types": "^0.30.0",
    "discord.js": "^13.7.0",
    "discordx": "^9.6.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.30.6",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/sharp": "^0.30.4",
    "@types/xmldom": "^0.1.34",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=7.0.0"
  }
}
